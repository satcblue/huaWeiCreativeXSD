<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="https://www.satcblue.cn/XMLSchema/HuaWeiCreative"
           xmlns="https://www.satcblue.cn/XMLSchema/HuaWeiCreative"
           elementFormDefault="qualified">
    <xs:include schemaLocation="../../complexType/2DBaseAnimation/AlphaAnimation.xsd"/>
    <xs:include schemaLocation="../../complexType/2DBaseAnimation/Alpha.xsd"/>

    <xs:element name="AlphaAnimation">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="AlphaAnimation">
                    <xs:choice minOccurs="0" maxOccurs="unbounded">
                        <xs:element name="Alpha" minOccurs="0"
                                    maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="Alpha">
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>
                    </xs:choice>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

</xs:schema>