<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="https://www.satcblue.cn/XMLSchema/HuaWeiCreative">
    <xs:include schemaLocation="../../complexType/2DBaseAnimation/SourcesAnimation.xsd"/>
    <xs:include schemaLocation="../../complexType/2DBaseAnimation/Source.xsd"/>

    <xs:element name="SourcesAnimation">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="SourcesAnimation">
                    <xs:choice minOccurs="0" maxOccurs="unbounded">
                        <xs:element name="Source" minOccurs="0"
                                    maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="Source">
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>
                    </xs:choice>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>


</xs:schema>