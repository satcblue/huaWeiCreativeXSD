<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="https://www.satcblue.cn/XMLSchema/HuaWeiCreative">

    <xs:include schemaLocation="../../complexType/view/Linear.xsd"/>
    <xs:include schemaLocation="../../element/view/GradientPoint.xsd"/>

    <xs:element name="Linear">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="Linear">
                    <xs:choice minOccurs="0" maxOccurs="unbounded">
                        <xs:element ref="GradientPoint" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:choice>
                </xs:extension>
            </xs:complexContent>

        </xs:complexType>
    </xs:element>

</xs:schema>
