<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="https://www.satcblue.cn/XMLSchema/HuaWeiCreative">
    <xs:include schemaLocation="../../complexType/control/NormalState.xsd"/>

    <xs:include schemaLocation="../../element/view/Image.xsd"/>
    <xs:include schemaLocation="../../element/view/Text.xsd"/>
    <xs:include schemaLocation="../../element/view/SourceImage.xsd"/>
    <xs:include schemaLocation="../../element/view/Time.xsd"/>
    <xs:include schemaLocation="../../element/view/DateTime.xsd"/>
    <xs:include schemaLocation="../../element/view/ImageNumber.xsd"/>

    <xs:include schemaLocation="../../element/group/Group.xsd"/>

    <xs:include schemaLocation="../../element/control/Trigger.xsd"/>

    <xs:include schemaLocation="../../element/command/CollBodyCommand.xsd"/>
    <xs:include schemaLocation="../../element/command/Command.xsd"/>
    <xs:include schemaLocation="../../element/command/CycleCommand.xsd"/>
    <xs:include schemaLocation="../../element/command/ExternCommand.xsd"/>
    <xs:include schemaLocation="../../element/command/GroupCommands.xsd"/>
    <xs:include schemaLocation="../../element/command/IntentCommand.xsd"/>
    <xs:include schemaLocation="../../element/command/ParticleCommand.xsd"/>
    <xs:include schemaLocation="../../element/command/RefreshWeatherCommand.xsd"/>
    <xs:include schemaLocation="../../element/command/SoundCommand.xsd"/>
    <xs:include schemaLocation="../../element/command/VariableCommand.xsd"/>
    <xs:include schemaLocation="../../element/command/VibrateCommand.xsd"/>
    <xs:include schemaLocation="../../element/command/VideoCommand.xsd"/>


    <xs:element name="NormalState">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="NormalState">
                    <xs:choice minOccurs="0" maxOccurs="unbounded">
                        <xs:element ref="Image" minOccurs="0" maxOccurs="unbounded"/>
                        <xs:element ref="Text" minOccurs="0" maxOccurs="unbounded"/>
                        <xs:element ref="SourceImage" minOccurs="0" maxOccurs="unbounded"/>
                        <xs:element ref="Time" minOccurs="0" maxOccurs="unbounded"/>
                        <xs:element ref="DateTime" minOccurs="0" maxOccurs="unbounded"/>
                        <xs:element ref="ImageNumber" minOccurs="0" maxOccurs="unbounded"/>

                        <xs:element ref="Group" minOccurs="0" maxOccurs="unbounded"/>

                        <xs:element ref="Trigger" minOccurs="0" maxOccurs="unbounded"/>

                        <xs:element ref="CollBodyCommand" minOccurs="0" maxOccurs="unbounded"/>
                        <xs:element ref="Command" minOccurs="0" maxOccurs="unbounded"/>
                        <xs:element ref="CycleCommand" minOccurs="0" maxOccurs="unbounded"/>
                        <xs:element ref="ExternCommand" minOccurs="0" maxOccurs="unbounded"/>
                        <xs:element ref="GroupCommands" minOccurs="0" maxOccurs="unbounded"/>
                        <xs:element ref="IntentCommand" minOccurs="0" maxOccurs="unbounded"/>
                        <xs:element ref="ParticleCommand" minOccurs="0" maxOccurs="unbounded"/>
                        <xs:element ref="RefreshWeatherCommand" minOccurs="0" maxOccurs="unbounded"/>
                        <xs:element ref="SoundCommand" minOccurs="0" maxOccurs="unbounded"/>
                        <xs:element ref="VariableCommand" minOccurs="0" maxOccurs="unbounded"/>
                        <xs:element ref="VibrateCommand" minOccurs="0" maxOccurs="unbounded"/>
                        <xs:element ref="VideoCommand" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:choice>
                </xs:extension>
            </xs:complexContent>

        </xs:complexType>
    </xs:element>
</xs:schema>